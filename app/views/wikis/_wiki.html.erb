<div class="bs-example" data-example-id="hoverable-table">
  <table class="table table-hover">
    <thead>
    <tr>
      <th>Programming</th>
      <th>Edit</th>
      <th>Delete</th>
      <th>Author</th>
    </tr>
    </thead>
    <tbody>
      <% @wikis.each do |wiki| %>
        <tr>
          <td><%= link_to wiki.title, wiki %></td>
          <td><%= link_to "Edit", edit_wiki_path(wiki.id), class: 'btn btn-primary btn-xs' %></td>
          <td><%= link_to "Delete", wiki_path(wiki.id), class: 'btn btn-danger btn-xs', method: :delete, data: { confirm: "Are you sure?" } %></td>
          <td><%= wiki.username %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div><!-- /example -->

